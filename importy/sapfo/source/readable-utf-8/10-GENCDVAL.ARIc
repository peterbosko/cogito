%   S A P F O   -   Sense APprehending Frame Operator   -   (c) Emil P·leö%                                                                 1988  %                 (part 10 : C,D-VALENCY, CONSTRUCTION):- segment(seglim).:- public colfar/5,leftsurf/7,rightsurf/7:far.:- extrn attach/3,member/2,pick/4,pair/2,int/6,int4/3,leftval/5,      cat3/4,prior/4,sema/2,retaxon/2,casecat/2,retroot/2,prep2/6:far.:- extrn radix/4:far.colfar(NN,N,L,LL,_) :-    ifthenelse(recorded(cfchart,clfr(NN,N,L,LL,_),_),fail,    (colfar0(NN,N,L,LL,Ret),     recordz(cfchart,clfr(NN,N,L,LL,Ret),_),fail)).colfar(NN,N,L,LL,Ret) :- recorded(cfchart,clfr(NN,N,L,LL,Ret),_).colfar0(NN,N,L,LL,Ret) :-   member([N,KO|L0],LL),!,   ifthenelse(NN=nil,L1=L0,pick(L0,_,NN,L1)),   ifthenelse(L1=[],     ((L=[N,KO],Ret=[]);      (colfar3(KO,KM,Ret),L=[N,KM])),     ((config(_,kon(_,_,a,_),_,_,_,_,_,_),         % KO=(_,0),       ((pick(L1,ifr,('ûen',0),L2),KO2=('ûen',0));	(KO=(osob,0),config(_,_,_,_,_,ine(a,_,_,_,_),_,_),L2=L1,KO2=('ûen',0));        (KO2=KO,L2=L1)       ),       pick(L2,SP,M,L3),integer(M),L=[N,KM|L3],       colfar0(N,M,[M,K|L4],LL,Ret1),       (L5=L3),       colfar1(KO2,SP,K,L5,KM1,Ret2),       ((Ret4=[],KM=KM1);colfar3(KM1,KM,Ret4)),       append(Ret1,Ret2,Ret3),append(Ret3,Ret4,Ret));      (pick(L1,SP,M,L3),integer(M),L=[N,KM|L3],       % KO\=(_,0),       colfar0(N,M,[M,K|_],LL,Ret1),colfar2(KO,SP,K,L3,KM,Ret2),       append(Ret1,Ret2,Ret));      (colfar3(KO,KM,Ret),L=[N,KM|L0]);		       % KO\=(_,0),      (pick(L1,Sp,Val,L3),atom(Val),Sp\==neg,Sp\==cas,       colfar4(KO,Sp,Val,L3,L4,KM,Ret),L=[N,KM|L4]);      (config(_,kon(_,_,_,a),_,_,_,_,_,_),pick(L1,SP,M,L2),     % KO=(_,0),       colfar0(N,M,[M,K|_],LL,Ret1),colkonfar1(KO,SP,K,M,LL,L2,L3,KM,Ret2),       append(Ret1,Ret2,Ret),L=[N,KM|L3]);      (config(_,kon(_,_,_,a),_,_,_,_,_,_),		   % KO\=(_,0),       (radix(KM,(KO,_),_,_);radix(KM,(_,KO),_,_)),       pick(L1,SP,M,L2),       colfar0(N,M,[M,K|_],LL,Ret1),colkonfar2(KO,SP,K,L2,L3,KM,Ret2),       append(Ret1,Ret2,Ret),L=[N,KM|L3]);      (radix(KO,nil,_,_),L=[N,KO|L0],Ret=[]);      (radix(KO,_,adv,_),L=[N,KO|L0],Ret=[])    )).% SUB -> SUB% rybnik·r,lesnÌk,zemepisec,asfaltÈrcolfar1((osob,0),(oku,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([osoba|_],'',Sf,_,_)),   (Sf='·r';Sf=ec;Sf='nÌk';Sf='ËÌk';Sf='Ìk';Sf=ik;Sf=ar;Sf=iar;Sf=ista;    Sf='Èr';Sf=ca),   radix(KO,_,sub,_).% rezb·r,n·vrh·rcolfar1((osob,0),XSP,KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([osoba|_],_,Sf,_,_)),   (Sf='';Sf='nÌk';Sf=ca;Sf='·r';Sf=iar;Sf=ar;Sf=ant),   radix(KO,KOR,sub,_),   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),pair(XSP,SP),prior(I,[osoba],sub,SP).% z·hor·kcolfar1((osob,0),(ori,No),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([osoba|_],Pf,Sf,_,_)),   (Sf='';Sf='Ëan';Sf=an;Sf='·r';Sf=iar;Sf=ar;Sf='·k';Sf=iak;Sf=ak),   radix(KO,_,sub,cat(lok,_,_,_,_)),int4(Pf,lok,No).% liöiakcolfar1((osob,0),(rel,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([osoba|_],_,Sf,_,_)),   (Sf=iak;Sf=ak;Sf='·k';Sf='oö'),   radix(KO,_,sub,_).% manichejeccolfar1((osob,0),(koh,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([osoba|_],_,Sf,_,_)),   (Sf=ita;Sf=ec;Sf='·k';Sf=iak;Sf=ak;Sf=ista),   radix(KO,_,sub,_).% susedkacolfar1(('ûen',0),SP,KO,L,KM,['Fem']) :-   radix(KM,KO,sub,cat([osoba|_],_,Sf,_,_)),   (Sf='';Sf=ka;Sf='iËka';Sf=ina;Sf='yÚa';Sf=ica;Sf='ovn·';Sf='aÚa';Sf='ezn·'),   radix(KO,_,sub,cat([osoba|_],_,_,_,_)).% vl·kninacolfar1(('l·tk',0),(Sp,0),KO,L,KM,['Cat']) :-   (Sp=mot;(Sp=rel,Se=[mat|_]);(Sp=suk,Se=[zviera|_])),   radix(KM,KO,sub,cat([mat|_],_,Sf,_,_)),   (Sf=ina;Sf=ovina),   radix(KO,_,sub,cat(Se,_,_,_,_)),ifthen(Sp=mot,sema(Se,[kon])).% kotlinacolfar1((miest,0),(rel,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([lok|_],_,Sf,_,_)),   (Sf=ina;Sf=ovina),   radix(KO,_,sub,cat(Se,_,_,_,_)),sema(Se,[kon]).% zvonicacolfar1((miest,0),(xlok,_),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([lok|_],_,Sf,_,_)),   (Sf=ica;Sf='·Ë';Sf='nÌk';Sf=stvo;Sf=tvo;Sf='eÚ';Sf='·reÚ';Sf='iareÚ';    Sf='Úa';Sf='ovÚa';Sf=isko;Sf=ovisko;Sf=nica;Sf='Ìn';Sf=inec),   radix(KO,_,sub,_).% prsteÚ,zemiak,n·prsenkacolfar1(KK,(lok,No),KO,L,KM,['Cat']) :-   retaxon(KK,KM,Se),   radix(KM,KO,sub,cat(Se,Pr,Sf,_,_)),   (Sf=ka;Sf=nica;Sf=enka;Sf=ok;Sf='nÌk';Sf='·k';Sf=iak;Sf=ak;Sf='eÚ'),   radix(KO,_,sub,cat(Se2,_,_,_,_)),(Se2=[vec|_];Se2=[lok|_]),   int4(Pr,lok,No).% sÌrancolfar1(KK,(koh,0),KO,L,KM,['Cat']) :-   retaxon(KK,KM,S),sema(S,[kon]),   radix(KM,KO,sub,cat(S,_,Sf,_,_)),   (Sf='nÌk';Sf=nica;Sf='eæ';Sf=an;Sf=ec;Sf='Ìk'),   radix(KO,_,sub,_).% plech·Ëcolfar1((vec,0),(mat,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat(Se,_,Sf,_,_)),sema(Se,[kon]),   Sf='·Ë',   radix(KO,_,sub,cat([mat|_],_,_,_,_)).% Zadunajskocolfar1((miest,0),(lok,No),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([lok|_],Pr,Sf,_,_)),   (Sf=sko;Sf=ie),   radix(KO,_,sub,cat([lok|_],_,_,_,_)),int4(Pr,lok,No).% predlaktiecolfar1((vec,0),(lok,No),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([korp|_],Pr,Sf,_,_)),   Sf=ie,   radix(KO,_,sub,cat([korp|_],_,_,_,_)),int4(Pr,lok,No).% SL -> SUB% tul·k,vl·dca,strelec + cestuj˙ci + zbohatlÌk + pijancolfar1((osob,0),XSP,KO,L,KM,['Cat']) :-   radix(KO,_,sl,cat(I,_,_,_,_,_,_)),pair(XSP,SP),prior(I,[osoba],sub,SP),   radix(KM,KO,sub,cat([osoba|_],Pr,Sf,_,_)),   ((Sf=ec;Sf=ca;Sf='Ë';Sf='iË';Sf=iak;Sf='·k';Sf=ak;Sf='Èr';Sf=ant;Sf='nÌk';     Sf='ËÌk';Sf='·r';Sf=iar;Sf='aË';Sf='teæ');    (Sf='˙ci',pick([_,_],cas,'prÈzens',_));    ((Sf='lÌk';Sf=alec;Sf=ilec),pick([_,_],cas,'minul˝',_));    ((Sf=an;Sf='oö';Sf=aj),pick([_,_],mod,zvyk,L2))   ).% krytina,murivocolfar1((vec,0),(mot,0),KO,L,KM,['Cat']) :-   radix(KO,_,sl,_),   radix(KM,KO,sub,cat([mat|_],_,Sf,_,_)),   (Sf=ina;Sf=ivo;Sf=dlo;Sf='aË').% peËivocolfar1((vec,0),XSP,KO,L,KM,['Cat']) :-   radix(KO,_,sl,cat(I,_,_,_,_,_,_)),   radix(KM,KO,sub,cat([mat|_],_,Sf,_)),   Sf=ivo,pair(XSP,SP),prior(I,[mat],obj,SP).% Ëerpadlocolfar1(('n·stroj',0),(xins,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([ins|_],_,Sf,_,_)),   radix(KO,_,sl,_),   (Sf=dlo;Sf='Ë';Sf='iË';Sf='·Ë';Sf='aËka';Sf='·k';Sf='Ìk';Sf=ik;Sf=iak).% klenbacolfar1((vec,0),(xrez,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat(Se,_,Sf,_,_)),sema(Se,[kon]),sema(Se,[neziv]),   (Sf='';Sf=ba;Sf=ka),   radix(KO,_,sl,_).% strelnicacolfar1((miest,0),(xlok,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([lok|_],_,Sf,_,_)),   (Sf='eÚ';Sf='·reÚ';Sf='iareÚ';Sf='Úa';Sf='ovÚa';Sf=nica;Sf='iöte';    Sf=isko;Sf=ovisko;Sf=nisko;Sf=anisko;Sf=dlo;Sf=a),   radix(KO,_,sl,_).% SL(PART) -> SUB% zr˙canina,skamenelinacolfar1((vec,0),XSP,KO,L,KM,['Cat']) :-   radix(KO,_,sl,cat(I,_,_,_,_,_,_)),   radix(KM,KO,sub,cat(Se,_,Sf,_)),sema(Se,[kon]),pair(XSP,SP),   ((Sf=elina,prior(I,Se,sub,SP));    ((Sf=anina;Sf=enina;Sf=enka),prior(I,Se,obj,SP))).% muËenÌkcolfar1((osob,0),XSP,KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([osoba|_],_,Sf,_,_)),   (Sf='enÌk';Sf=atec),   radix(KO,_,sl,cat(I,_,_,_,_,_,_)),pair(XSP,SP),prior(I,[osoba],obj,SP).% sp·leniskocolfar1((miest,0),XSP,KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([lok|_],_,Sf,_,_)),   Sf=enisko,   radix(KO,_,sl,cat(I,_,_,_,_,_,_)),pair(XSP,SP),prior(I,[lok],obj,SP).% ADJ -> SUB% slepec,strieborniak + Ëernochcolfar1(KK,(atr,0),KO,L,KM,['Cat']) :-   retaxon(KK,KM,Se),   radix(KM,KO,sub,cat(Se,_,Sf,_,_)),   ((Sf='·Ë';Sf=ec;Sf='nÌk';Sf='·k';Sf=ak;Sf=iak;Sf='Ìk';Sf=an;     Sf='uöa';Sf=ucha;Sf='uæa';Sf=ena;Sf=ina);    ((Sf='·Ë';Sf='·Ú';Sf='oÚ';Sf='·ö';Sf='·æ';Sf='och'),     pick(L,mod,charakt,_))   ),   radix(KO,_,adj,_).% husacinacolfar1(('l·tk',0),(suk,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([mat|_],_,Sf,_,_)),   Sf=ina,   radix(KO,_,adj,cat(_,drprivl,_,_,_,_)).% franc˙zötinacolfar1((jazyk,0),(atr,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([gnoz|_],_,Sf,_,_)),   (Sf=ina;Sf='Ëina';Sf='ötina'),   radix(KO,_,adj,cat(_,akvzt,_,_,_,_)).% drev·rstvocolfar1((jav,0),(koh,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sub,cat([jav|_],_,Sf,_,_)),   (Sf=stvo;Sf=ctvo;Sf=tvo),   radix(KO,_,sub,_).% zlatovl·ska,bielohl·vok,voÚavkacolfar1((Nam,0),XSP,KO,L,KM,['Cat','Nol','Kdz']) :-   pair(XSP,SP),   leftsurf(1,(Nam,0),SP,Acon,AK,Seak,[[1,KO]]),   radix(AK,_,adj,cat(_,akvzt,_,Sf1,_,_)),   radix(KM,AK,sub,cat(Se,_,Sf2,_,_)),   (Sf2=ka;Sf2=ok;Sf2='aÚ').% SUB -> SL% kraæovaùcolfar1((by,0),(ifr,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sl,cat(400,'',Sf,_,nedok,_,_)),   (Sf='ovaù';Sf='Ëiù';Sf='iù'),   radix(KO,_,sub,cat([osoba|_],_,_,_,_)).% soliù,olejovaù,striebriù,hnojiù,v·pniù,mastiù,voskovaù,koreniù,farbiù% ozubiù,oöatiù,ovenËiù,okrÌdliù% kr˙ûkovaù% telefonovaù,bicyklovaù,loÔkovaù sa,lyûovaù,veslovaù,gumovaù,valcovaù,% kosiù,biËovaù,brzdiù,r˝æovaù,kr˙ûiùcolfar1(KK,SP,KO,L,KM,['Cat']) :-   radix(KM,KO,sl,cat(I,_,Sf,_,_,_,_)),   retaxon(KK,KM,I),   radix(KO,_,sub,cat(Se,_,_,_,_)),   (Sf='ovaù';Sf='iù'),   prior(I,Se,spe,SP).% ADJ -> SL% kr·öliùcolfar1((rob,0),(atr,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sl,cat(139,'',Sf,_,nedok,_,_)),   Sf='iù',   radix(KO,_,adj,cat(_,kval,_,_,_,_)),   pick(L,asp,nedok,_).% kr·snieùcolfar1((st,0),(atr,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sl,cat(140,'',Sf,_,nedok,_,_)),   (Sf='ieù';Sf='n˙ù'),   radix(KO,_,adj,_),pick(L,asp,nedok,_).% belieù sacolfar1((by,0),(atr,0),KO,L,KM,['Cat']) :-   radix(KM,KO,sl,cat(141,'',Sf,_,nedok,_,_)),   (Sf='ieù';Sf='aù'),   radix(KO,_,adj,_),pick(L,asp,nedok,_).% kamenieùcolfar1((st,0),SP,KO,L,KM,['Cat']) :-   radix(KM,KO,sl,cat(140,'',Sf,_,nedok,_,_)),   (Sf='ieù';Sf='n˙ù'),   radix(KO,_,sub,cat(Se,_,_,_,_)),   ifthenelse(Se=mat,SP=(mat,0),SP=(ifr,0)),   pick(L,asp,nedok,_).% 2 SUB -> SUB% sÙv‰ + Ëlnokcolfar2(KOR,(atr,0),(mal,1),L,KM,['Dem']) :-   radix(KM,KOR,sub,cat(Se1,_,Sf,_,_)),   (((Sf=a;Sf='‰';Sf='Ëa'),(Se=[osoba|_];Se=[zviera|_]));    (Sf=ok;Sf='Ìk';Sf=ik;Sf=ec;Sf='Ëek';Sf=ek;Sf=tek;Sf='ÌËek';Sf='ieËko';     Sf='eËko';Sf='oËko';Sf='ÌËko';Sf='Ëok';Sf=ka;Sf='iËka';Sf='oËka';     Sf='ÙËka';Sf=enka;Sf=ienka;Sf=inka;Sf=ko;Sf=ce;Sf=inko;Sf=enko)   ),   radix(KOR,_,sub,cat(Se2,_,_,_,_)).% jelöinacolfar2(KOR,(mes,0),('veæa',1),L,KM,['Kol']) :-   radix(KM,KOR,sub,cat([Se|_],_,Sf,_,hrom)),   (Sf=ina;Sf=ie;Sf=stvo;Sf='iz·cia';Sf='aù';Sf='eù';Sf='aË';Sf='iË'),   radix(KOR,_,sub,cat([Se|_],_,_,_,_)).% polmesiac + stromiskocolfar2(KOR,(atr,0),AK,L,KM,Ret) :-   radix(KM,KOR,sub,cat([Se|_],Pr,Sf,_,_)),   ((AK=('poloviËn',1),(Pr=pol;Pr=polo),Ret=['Cat']);    (AK=('veæk',1),((Pr='veæ',Sf='');(Pr='',Sf=isko)),Ret=['Mag'])   ),   radix(KOR,_,sub,cat([Se|_],_,_,_,_)).% s˙hvezdie, s˙soöiecolfar2(KOR,(kar,0),('veæa',1),L,KM,['Kol']) :-   radix(KM,KO,sub,cat(_,'s˙',ie,_,_)).% ûltovlas˝colfar2(KOR,(atr,0),AK,L,KM,['Kdz']) :-   radix(KM,(AK,KOR),adj,_),   radix(AK,_,adj,_),   radix(KOR,_,sub,_).% 3 SUB -> SUB%draËicacolfar3(KO,KM,['Fem']) :-   config(_,_,_,_,_,ine(a,_,_,_,_),_,_),   radix(KM,KO,sub,cat(Se,Pr,Sf,Vz1,_)),   (Se=[osoba|_];Se=[zviera|_]),   (Sf='';Sf=ka;Sf='iËka';Sf=ina;Sf='yÚa';Sf=ica;Sf='ovn·';Sf='aÚa';Sf='ezn·'),   radix(KO,_,sub,cat(Se,_,_,Vz2,_)),   cdm,vzorsub(Vz2,muz,_),vzorsub(Vz1,zen,_),cdx.% studnicacolfar3(KO,KM,['Dub']) :-   radix(KM,KO,sub,cat(Se,_,Sf,Vz1,_)),   (Sf=ica;Sf=nica;Sf=ka;Sf=ce;(Sf=ina,Se\=mat);Sf=''),   radix(KO,_,sub,cat(Se,_,_,Vz2,_)),   cdm,ifthen((vzorsub(Vz2,muz,_),vzorsub(Vz1,zen,_)),fail),cdx.% 3 SL -> SUB% vrt,cengotcolfar3(KO,KM,['Nom']) :-   radix(KM,KO,sub,cat([dej|_],Pr,Sf,_,_)),   (Sf='';Sf=ot;Sf=ba;Sf=oba;Sf=ota;Sf='aËka';Sf=ka;Sf=ica;Sf=ina;Sf='·cia';    Sf=a;Sf=anica;Sf=enica;Sf=ie;Sf='osù'),   radix(KO,_,sl,_).% 3 ADJ -> SUB% ËerÚcolfar3(KO,KM,['Nom']) :-   radix(KM,KO,sub,cat([atb|_],Pf,Sf,_,_)),   (Sf='';Sf=a;Sf='osù';Sf=oba;Sf=ota;Sf=tvo;Sf=ctvo;Sf=stvo;Sf=ava;Sf=izmus),   (Pf='';Pf=ne;Pf=polo;Pf=bez),   radix(KO,_,adj,_).% kr·snycolfar3(KO,KM,[]) :-   radix(KM,KO,adj,cat(_,Se,Pf,Sf,_,_)),   Sf='',   radix(KO,_,adj,cat(_,Se,_,_,_,_)).% trieù% colfar4(KO,asp,_,L,L,KO,[]) :-%   radix(KO,nil,_,_).% 4 SL -> SL% uloviùcolfar4(KO,asp,dok,L,L1,KM,['Asp']) :-   radix(KM,KO,sl,cat(I,Pr,'',_,dok,_,_)),   I>40,(Pr=u;Pr=vy;Pr=za;Pr=s;Pr=z;Pr=zo;Pr=pre;Pr=na;Pr=od;Pr=o),   radix(KO,_,sl,cat(_,_,_,_,nedok,_,_)),append(L,[asp,nedok],L1).% odniesùcolfar4(KO,dir,Pr,L1,L3,KM,['Dir']) :-    radix(KM,KO,sl,cat(I,Pr,'',_,dok,_,_)),   I<40,(Pr=od;Pr=do;Pr=pri;Pr=z;Pr=v;Pr=pre),   pick(L1,asp,dok,L2),   radix(KO,_,sl,cat(_,_,_,_,nedok,_,_)),append(L2,[asp,nedok],L3).% odn·öaùcolfar4(KO,asp,nedok,L,L2,KM,['Asp']) :-   radix(KM,KO,sl,cat(_,'',Sf,_,nedok,_,_)),   (Sf='aù';Sf='ievaù';Sf='ovaù'),   radix(KO,_,sl,cat(_,_,_,_,dok,_,_)),append(L,[asp,dok],L2).% ËÌtavaùcolfar4(KO,asp,opak,L,L2,KM,['Asp']) :-   radix(KM,KO,sl,cat(_,'',Sf,_,opak,_,_)),   (Sf='ievaù';Sf='avaù'),   radix(KO,_,sl,cat(_,_,_,_,nedok,_,_)),append(L,[asp,nedok],L2).% rozosmiaù,dofajËiùcolfar4(KO,faz,Faz,L,L,KM,['Faz']) :-   radix(KM,KO,sl,cat(_,Pr,'',_,dok,_,_)),   ((Faz=('zaË',1),(Pr=roz;Pr=rozo));    (Faz=(prest,1),Pr=do)   ).% narobiù sacolfar4(KO,(mes,0),('veæa',1),L,L,KM,['Mag']) :-   radix(KM,KO,sl,cat(_,na,'',sa,dok,_,_)),   radix(KO,_,sl,cat(_,_,_,_,nedok,_,_)).% vyspevovaùcolfar4(KO,(mes,0),('veæa',1),L,L,KM,['Mag']) :-   radix(KM,KO,sl,cat(_,Pr,Sf,_,Vd,_,_)),   ((Pr=vy,Sf='ovaù');(Pr=roz,Sf='iavaù');(Pr=pre,Sf='aù')),   radix(KO,_,sl,cat(_,_,_,_,Vd,_,_)).% spievkaù colfar4(KO,(mes,0),('m·l',1),L,L,KM,['Dem']) :-   radix(KM,KO,sl,cat(_,Pr,Sf,_,Vd,_,_)),   ((Pr=po,Sf='ovaù');    (Pr='',Sf='kaù')),   radix(KO,_,sl,cat(_,_,_,_,Vd,_,_)).% SUB+SUB -> SUB% delostreleccolkonfar1((osob,0),XSP1,KOR,M,LL,L,L,KM,['Kdz'|Ret]) :-   radix(KM,(KO,KOR),sub,cat([osoba|_],'','',_,_)),   radix(KOR,KORR,sub,cat([osoba|_],'',Sf,_,_)),   (Sf=ec;Sf=ca;Sf='Ë';Sf='iË';Sf=iak;Sf='·k';Sf=ak;Sf='Èr';Sf=ant;Sf='nÌk';    Sf='ËÌk';Sf='·r';Sf=iar;Sf='aË'),   radix(KORR,_,sl,cat(I,_,_,_,_,_,_)),   pair(XSP1,SP1),prior(I,[ziv],sub,SP1),   member([M,_KOR|L3],LL),pick(L3,SP2,M2,_),   colfar0(M,M2,[_,KO],LL,Ret),   radix(KO,_,sub,cat(S,_,_,_,_)),   prior(I,S,nesub,SP2).% lesostep,ötrkopiesokcolkonfar1(KK,(ifr,0),KOR1,_,LL,L1,L2,KM,['Kdz']) :-   retaxon(KK,KM,Se),   radix(KM,(KOR1,KOR2),sub,cat([S|_],'','',_,_)),   radix(KOR1,_,sub,cat([S|_],_,_,_,_)),   radix(KOR2,_,sub,cat([S|_],_,_,_,_)),   pick(L1,(ifr,0),M,L2).% SUB+SL -> SUB% lodivodcolkonfar1((osob,0),XSP1,KOR,M,LL,L,L,KM,['Kdz'|Ret]) :-   radix(KM,(KO,KOR),sub,cat([osoba|_],'',F,_,_)),   (F='';F='aË';F=ec;F=ca;F='·r';F=ar;F=iar;F='Ìk'),   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),   pair(XSP1,SP1),prior(I,[ziv],sub,SP1),   member([M,_|L3],LL),pick(L3,SP2,M2,_),   colfar0(M,M2,[_,KO],LL,Ret),   radix(KO,_,sub,cat(S,_,_,_,_)),   prior(I,S,nesub,SP2).% k·vovarcolkonfar1(('n·stroj',0),(xins,0),KOR,M,LL,L,L,KM,['Kdz'|Ret]) :-   radix(KM,(KO,KOR),sub,cat([ins|_],'',Sf,_,_)),   (Sf='';Sf='iË';Sf=ka),   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),   member([M,_|L3],LL),pick(L3,SP2,M2,_),   colfar0(M,M2,[_,KO],LL,Ret),   radix(KO,_,sub,cat(S,_,_,_,_)),   prior(I,S,_,SP2).% pivovarcolkonfar1((miest,0),(xlok,No),KOR,M,LL,L,L,KM,['Kdz'|Ret]) :-   radix(KM,(KO,KOR),sub,cat([lok|_],'',Sf,_,_)),   (Sf='';Sf='·reÚ';Sf='iareÚ'),   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),   member([M,_|L3],LL),pick(L3,SP2,M2,_),   colfar0(M,M2,[_,KO],LL,Ret),   radix(KO,_,sub,cat(S,_,_,_,_)),   prior(I,S,_,SP2).% ADJ+SUB -> SUB% hrubokoûeccolkonfar1(KK,(xpar,0),KOR,M,LL,L,L,KM,['Kdz']) :-   retaxon(KK,KM,Se),   radix(KM,(KO,KOR),sub,cat(Se,'',Sf,_,_)),   (Sf='';Sf=ec;Sf=ok;Sf=ka),   member([M,_|L3],LL),pick(L3,(atr,0),M2,_),member([M2,KO|_],LL),   radix(KOR,_,sub,cat([korp|_],_,_,_,_)),   radix(KO,_,adj,_).% SL+SUB -> SUB% tlËhubacolkonfar1((osob,0),XSP1,KOR,M,LL,L,L,KM,['Kdz']) :-   radix(KM,(KOR,KO),sub,cat([osoba|_],'',Sf,_,_)),   (Sf='';Sf=a),   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),   pair(XSP1,SP1),prior(I,[ziv],sub,SP1),   member([M,_|L3],LL),pick(L3,SP2,M2,_),   colfar0(M,M2,[_,KO],LL,Ret),   radix(KO,_,sub,cat(S,_,_,_,_)),   prior(I,S,_,SP2).% CIS+SUB -> SUB% osemuholnÌkcolkonfar1(KK,(xpar,0),KOR,M,LL,L,L,KM,['Kdz']) :-   retaxon(KK,KM,Se),   radix(KM,(KO,KOR),sub,cat(Se,'',Sf,_,_)),   (Sf='';Sf='nÌk'),   radix(KO,_,kar,_),   member([M,_|L3],LL),pick(L3,kar,M2,_).% 2 SUB+SL -> SUB% cestopiscolkonfar2(KOR,XSP,KO,L1,L2,KM,['Kdz']) :-   radix(KM,(KO,KOR),sub,_),   radix(KO,_,sub,cat(S,_,_,_,_)),   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),   pair(XSP,SP),prior(I,S,ord,SP).% rybolovcolkonfar2(KOR,XSP,KO,L1,L2,KM,['Kdz']) :-   radix(KM,(KO,KOR),sub,cat([dej|_],_,'',_,_)),   radix(KO,_,sub,cat(S,_,_,_,_)),   radix(KOR,KORR,sub,cat([dej|_],_,Sf,_,_)),   (Sf='';Sf=a;Sf=ba;Sf=oba;Sf=ota),   radix(KORR,_,sl,cat(I,_,_,_,_,_,_)),   pair(XSP,SP),prior(I,S,_,SP).% 2 ADJ+SUB -> SUB% Starhrad,Krasopannacolkonfar2(KOR,(atr,0),KO,L,L,KM,['Kdz']) :-   radix(KM,(KO,KOR),sub,cat(Se,'','',_,_)),   radix(KO,_,adj,_),   radix(KOR,_,sub,cat(_,_,_,_,_)).% ADJ+SL -> SUB% krasopiscolkonfar2(KOR,(mds,0),KO,L,L,KM,['Kdz']) :-   radix(KM,(KO2,KOR),sub,cat(_,'','',_,_)),   radix(KO,_,sl,_),   radix(KO,KO2,adv,_),   radix(KO2,_,adj,_).retaxon(KM,_,[Se|_]) :- feaname(Se,KM),!.retaxon(KM,_,I) :- maxtaxon(I,KM).retaxon(K1,K2,Se) :- abstr(K2,K1).maxtaxon(I,(pohybov,0)) :- (I=1;I=2),!.maxtaxon(20,(rob,0)) :- !.