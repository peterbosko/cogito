,XSP2),   ctr_inc(1,X),ctr_inc(1,Y),ctr_inc(1,Z),   L=[[X,(osob,0),XSP1,Y],[Y,KO2,SP1,X,SP2,Z],[Z,K1,XSP2,Y]].% drevorubaË,lodivodkonval(lk(sub,S1,_,K1),rk(sl,I,_,K2),kk(sub,[osoba|_],F,_),L) :-   (F='';F='aË';F=ec;F=ca;F='·r';F=ar;F=iar;F='Ìk'),   prior(I,[ziv],sub,SP1),pair(SP1,XSP1),   prior(I,S1,nesub,SP2),pair(SP2,XSP2),   ctr_inc(1,X),ctr_inc(1,Y),ctr_inc(1,Z),   L=[[X,(osob,0),XSP1,Y],[Y,K2,SP1,X,SP2,Z],[Z,K1,XSP2,Y]].% ropovod,k·vovar,mucholapka,vetrolam,tlakomerkonval(lk(sub,S1,_,K1),rk(sl,I2,_,K2),kk(sub,Se2,F,K3),L) :-   (F='';F='iË';F=ka),(Se2=[ins|_];Se2=[stroj|_]),   prior(I2,S1,_,SP),pair(SP,XSP),   ctr_inc(1,X),ctr_inc(1,Y),ctr_inc(1,Z),taxon(K3,Se2,Nam),   L=[[X,Nam,(xins,0),Y],[Y,K2,(ins,0),X,SP,Z],[Z,K1,XSP,Y]].% pivovar,kameÚolomkonval(lk(sub,S1,_,K1),rk(sl,I2,_,K2),kk(sub,[lok|_],F,K3),L) :-   (F='';F='·reÚ';F='iareÚ'),   prior(I2,S1,_,SP),pair(SP,XSP),   ctr_inc(1,X),ctr_inc(1,Y),ctr_inc(1,Z),   L=[[X,(miest,0),(xlok,No),Y],[Y,K2,(lok,No),X,SP,Z],[Z,K1,XSP,Y]].% elektrospotrebiË% knÌhtaËiareÚ% ADJ+SUB -> SUB% hrubokoûeckonval(lk(adj,kval,_,K1),rk(sub,[korp|_],_,K2),kk(sub,S,F,K3),L) :-   (F='';F=ec;F=ka;F=ok),sema(S,[ziv]),   ctr_inc(1,X),ctr_inc(1,Y),ctr_inc(1,Z),taxon(K3,S,Nam),   L=[[X,Nam,par,Y],[Y,K2,xpar,X,(atr,0),Z],[Z,K1,x(atr,0),Y]].% Starhrad,krasopanna,Krivokl·t,kozoroh,zloduch,novomanûel,% krasojazda,drahokamkonval(lk(adj,_,_,K1),rk(sub,_,_,K2),kk(sub,_,'',_),L) :-   leftval(K1,ano,poz,K2,L1),   disfar(K2,Q),attach(Q,L1,L).% SL+SUB -> SUB% tlËhuba,drûgroökonval(lk(sl,I1,_,K1),rk(sub,S2,_,K2),kk(sub,[osoba|_],F,K3),L) :-   (F='';F=a),   prior(I1,[ziv],sub,SP1),pair(SP1,XSP1),   prior(I1,S2,_,SP2),pair(SP2,XSP2),   ctr_inc(1,X),ctr_inc(1,Y),ctr_inc(1,Z),   L=[[X,('osob',0),XSP1,Y],[Y,K1,SP1,X,SP2,Z],[Z,K2,XSP2,Y]].% ADJ+SL -> SUB% krasopiskonval(lk(adj,_,_,K1),rk(sl,_,_,K2),kk(sub,_,'',_),L) :-   radix(K3,K1,adv,_),ctr_inc(1,X),   disfar(K2,Q),attach(Q,[mds,[X,K3]],L).% ostrostr