%   S A P F O   -   Sense APprehending Frame Operator   -   (c) Emil P·leö%                                                                 1988  %                   (part 8 :  C,D-VALENCY, ANALYSIS):- segment(segfol).:- public farfield/1,addfar/0,disfar/2:far.:- extrn leftval/5,int4/3,retroot/2,sema/2,prior/4,attach/3,attachh/3,     pair/2,feaname/2,vzorsub/3,retachh/4:far.:- extrn radix/4:far.farfield(KM) :- radix(KM,_,_,_),!.farfield(((Km1,N1),(Km2,N2))) :- !,farfield((Km1,N1)),farfield((Km2,N2)).farfield(KM) :-    data_world(W,lexicon),    retrieveb(dic,KM,kmen(_,_,KO,_)),    ifthenelse(KO==nil,farfield1(KM),farfield(KO)),   data_world(_,W),!.farfield1(KM) :- radix(KM,_,_,_),!.farfield1(KM) :- config(_,_,_,_,_,_,_,styl(St)),   retrieveb(dic,KM,kmen(Sd,Cat,KO,L)),   ifthen((styl(KM,S),S=St),(assertz((radix(KM,KO,Sd,Cat))),farfield2(L))).farfield2([]) :- !.farfield2([A|L]) :- farfield1(A),farfield2(L),!.farsynfield :- radix(K1,_,_,_),   (synon(K1,K2);synon(K2,K1);anton(K1,K2);anton(K2,K1);    konverz(K1,K2);konverz(K2,K1);inverz(K1,K2);inverz(K2,K1);    magnon(K1,K2);magnon(K2,K1);magnon2(K1,K2),magnon2(K2,K1);    symbol(K1,K2);symbol(K2,K1);bonif(K1,K2);bonif(K2,K1);    abstr(K1,K2);abstr(K2,K1);qual(K1,_,K2)    ),   farfield(K2),fail.farsynfield :- !.chgstyle :- radix(K,nil,_,_),recordz(stlchrt,K,_),fail.chgstyle :- [!abolish(radix/4),addfar!],   recorded(stlchrt,K,_),farfield(K),fail.chgstyle :- farsynfield,eraseall(stlchrt),expunge,!.addfar :- assertz((radix((by,0),nil,sl,cat(201,'','',nezv,nedok,'byù',som)))),   assertz((radix((st,0),(by,0),sl,cat(201,'','',sa,dok,'pracovaù',stan)))),   assertz((radix((rob,0),nil,sl,cat(207,'','',nezv,nedok,'robiù',rob)))),   assertz((radix((urob,0),(rob,0),sl,cat(207,u,'',nezv,dok,'robiù',urob)))),   assertz((radix((pohybov,0),nil,sl,cat(1,'','',sa,nedok,'pracovaù',pohybuj)))),   assertz((radix((zaober,1),nil,sl,cat(153,'','',sa,nedok,'chytaù',zaober)))),   assertz((radix((osob,0),nil,sub,cat(osoba,'','','ûena',poc)))),   assertz((radix((miest,0),nil,sub,cat(lok,'','','r˙cho',poc)))),   assertz((radix((jav,0),nil,sub,cat(jav,'','',dub,poc)))),   assertz((radix(('l·tk',0),nil,sub,cat(mat,'','','kr·ska',poc)))),   assertz((radix(('rastlin',0),nil,sub,cat(flor,'','','ûena',poc)))),   assertz((radix(('zvier',0),nil,sub,cat(zviera,'','','dievËa',poc)))),   assertz((radix(('vec',0),nil,sub,cat(vec,'','','kosù',poc)))),   assertz((radix(('veæk',1),nil,adj,cat(uni,kval,'','','pekn˝',suplet)))),   assertz((radix((mal,1),nil,adj,cat(uni,kval,'','','pekn˝',suplet)))).disfar1(KM,Q) :- ifthenelse(recorded(farchart,dsfr(KM,Q),_),fail,   (disfar1(KM,Q),recordz(farchart,dsfr(KM,Q),_),fail)).disfar1(KM,Q) :- recorded(farchart,dsfr(KM,Q),_).disfar(KM,Q) :- ctr_is(1,Z),radix(KM,KO,Sd,Cat),   case([Sd=sl -> Cat=cat(Se,Pr,Sf,_,Vd,_,_),         Sd=adj -> Cat=cat(_,Se,Pr,Sf,_,_)         |Cat=cat(Se,Pr,Sf,_,_)]),   ifthenelse(KO==nil,(ctr_inc(1,X),                       ifthenelse(Sd=sl,Q=[[X,KM,asp,Vd]],Q=[[X,KM]])),    ifthenelse(KO=((_,_),(_,_)),     (KO=(KO1,KO2),radix(KO1,_,Sd1,Cat1),radix(KO2,_,Sd2,Cat2),      ascarg(Sd1,Cat1,Se1,Sf1),ascarg(Sd2,Cat2,Se2,Sf2),      konval(lk(Sd1,Se1,Sf1,KO1),rk(Sd2,Se2,Sf2,KO2),kk(Sd,Se,Sf,KM),Q)),     ((radix(KO,_,Sd1,Cat1),      case([Sd1=sl -> Cat1=cat(Se1,_,_,_,Vd1,_,_),	    Sd1=adj -> Cat1=cat(_,Se1,_,_,_,_)            |Cat1=cat(Se1,_,_,_,_)]),       inval(km(Sd,Se,Pr,Sf,Vd,KM),ko(Sd1,Se1,Vd1,KO),Q)      );(ctr_is(1,Z),ctr_inc(1,X),         ifthenelse(Sd=sl,Q=[[X,KM,asp,Vd]],Q=[[X,KM]]))     ))    ).      % SL -> SUB% tul·k,vl·dcainval(km(sub,S,Pr,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf=ec;Sf=ca;Sf='Ë';Sf='iË';Sf=iak;Sf='·k';Sf=ak;Sf='Èr';Sf=ant;Sf='nÌk';    Sf='ËÌk';Sf='·r';Sf=iar;Sf='aË'),   prior(I,S,sub,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),   ifthenelse(Pr=ne,attachh(L1,[cas,'prÈzens',neg,nie],L2),                    attachh(L1,[cas,'prÈzens'],L2)),   attach([[X,(osob,0)]],[XSP|L2],L).% pijan,hraboöinval(km(sub,S,_,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf=an;Sf='˙Ú';Sf='oö';Sf=aj),sema(S,[ziv]),   prior(I,S,sub,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),attachh(L1,[mod,zvyk],L2),   attach([[X,(osob,0)]],[XSP|L2],L).% vrt,cvengot,paæba,oberaËka,cizel·cia% tlaËenica,posedenie,ûiadosù,mur·rËinainval(km(sub,[dej|_],Pr,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf='';Sf=ot;Sf=ba;Sf=oba;Sf=ota;Sf='aËka';Sf=ka;Sf=ica;Sf=ina;Sf='·cia';    Sf=anica;Sf=enica;Sf=ie;Sf='osù'),   disfar(KO,L).% krytina, kurivo,rozp˙öùadlo,odfarbovaËinval(km(sub,[mat|_],_,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf=ina;Sf=ivo;Sf=dlo;Sf='aË'),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(vec,0)]],[(mot,1)|L1],L).% peËivo			inval(km(sub,[mat|_],_,ivo,_,KM),ko(sl,I,_,KO),L) :-   prior(I,[mat],obj,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(vec,0)]],[XSP|L1],L).% Ëerpadlo,tlmiË,pilnÌk,mieöaËkainval(km(sub,[ins|_],_,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf=dlo;Sf='Ë';Sf='iË';Sf='·Ë';Sf='aËka';Sf='·k';Sf='Ìk';Sf=ik;Sf=iak;Sf=ak;Sf=nica),   disfar(KO,L1),ctr_inc(1,X),attach([[X,('n·stroj',0)]],[(xins,0)|L1],L).% n·syp,klenba,snÌmka,z·sielkainval(km(sub,S,_,Sf,_,KM),ko(sl,I,_,KO),L) :-    (Sf='';Sf=ba;Sf=ka),sema(S,[kon]),sema(S,[neziv]),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(vec,0)]],[(xrez,0)|L1],L).% strelnica,pracovisko,tlaËiareÚ,herÚa,v˝stavainval(km(sub,[lok|_],_,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf='eÚ';Sf='·reÚ';Sf=iareÚ;Sf='Úa';Sf='ovÚa';Sf=nica;Sf='iöte';    Sf=isko;Sf=ovisko;Sf=nisko;Sf=anisko;Sf=dlo;Sf=a),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(miest,0)]],[(xlok,_)|L1],L).% plav·k, visaËka, stojan, leûiak% rozpr·vka% SL(PART) -> SUB% cestuj˙ciinval(km(sub,S,_,'˙ci',_,KM),ko(sl,I,_,KO),L) :-   prior(I,S,sub,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),attachh(L1,[cas,'prÈzens'],L2),   attach([[X,(osob,0)]],[XSP|L2],L).% zbohatlÌk, povstalecinval(km(sub,S,_,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf='lÌk';Sf='alec';Sf=ilec),   prior(I,S,sub,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),attachh(L1,[cas,minuly],L2),   attach([[X,(osob,0)]],[XSP|L2],L).% muËenÌk,zajatecinval(km(sub,S,_,Sf,_,KM),ko(sl,I,_,KO),L) :-   (Sf='enÌk';Sf=atec),   prior(I,S,obj,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(osob,0)]],[XSP|L1],L).% zr˙canina,skamenelinainval(km(sub,S,_,Sf,_,KM),ko(sl,I,_,KO),L) :-  sema(S,kon),   ((Sf=elina,prior(I,S,sub,SP));    ((Sf=enina;Sf=anina;Sf=enka),   prior(I,S,obj,SP))),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),   attachh(L1,[cas,'minul˝'],L2),attach([[X,(vec,0)]],[XSP|L2],L).% sp·leniskoinval(km(sub,[lok|_],_,enisko,_,KM),ko(sl,I,_,KO),L) :-   prior(I,[lok],obj,SP),pair(SP,XSP),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(miest,0)]],[XSP|L1],L).% p·lenÈ% SUB -> SUB% lesnÌk,zemepisec,traktorista,asfaltÈrinval(km(sub,[osoba|_],_,Sf,_,KM),ko(sub,Se,_,KO),L) :-   (Sf='nÌk';Sf='ËÌk';Sf='Ìk';Sf=ik;Sf=ec;Sf='·r';Sf=ar;Sf=iar;    Sf=ista;Sf='Èr';Sf=ca),   ifthenelse((qualia(KO,SP2,K2),casecat(obj,SP2)),     true,K2=(zaober,0)),   ifthenelse(synekdocha(KO,K4),true,K4=KO),   ifthenelse(abstr(KM,K5),true,K5=(osob,0)),   radix(K2,_,sl,cat(I,_,_,_,_,_,_)),   prior(I,[ziv],sub,SP1),   prior(I,Se,obj,SP2),   disfar(K4,L1),ctr_inc(1,X),ctr_inc(1,Y),pair(SP1,XSP1),   attach([[Y,K2]],[SP2|L1],L2),attach([[X,K5]],[XSP1|L2],L).%  disfar(KO,L1),ctr_inc(1,X),attach([[X,(osob,0)]],[(oku,0)|L1],L).% rezb·r,n·vrh·r,projektant,zaËiatoËnÌkinval(km(sub,[osoba|_],_,Sf,_,KM),ko(sub,_,_,KO),L) :-   (Sf='';Sf='nÌk';Sf=ca;Sf='·r';Sf=iar;Sf=ar;Sf=ant),   disfar(KO,L1),L1=[[_,KOR|_]|_],   radix(KOR,_,sl,cat(I,_,_,_,_,_,_)),   prior(I,[osoba],sub,SP),pair(SP,XSP),   ctr_inc(1,X),attach([[X,(osob,0)]],[XSP|L1],L).% z·hor·k,GemerËan,Indinval(km(sub,[osoba|_],Pf,Sf,_,KM),ko(sub,[lok|_],_,KO),L) :-   (Sf='';Sf='Ëan';Sf=an;Sf='·r';Sf=iar;Sf=ar;Sf='·k';Sf=iak;Sf=ak),   int4(Pf,lok,No),disfar(KO,L1),ctr_inc(1,X),   attach([[X,(osob,0)]],[(ori,No)|L1],L).% liöiakinval(km(sub,[osoba|_],_,Sf,_,KM),ko(sub,_,_,KO),L) :-   (Sf=iak;Sf=ak;Sf='·k';Sf=oö),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(osob,0)]],[(rel,0)|L1],L).% manichejec,husita,zv‰z·k,doprav·kinval(km(sub,[osoba|_],Pf,Sf,_,KM),ko(sub,S,_,KO),L) :-   (Sf=ita;Sf=ec;Sf='·k';Sf=iak;Sf=ak;Sf=ista),(S=[org|_];S=[abs|_]),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(osob,0)]],[(koh,0)|L1],L).% susedka,geologiËka,bohyÚainval(km(sub,Se,_,Sf,_,KM),ko(sub,Se,_,KO),L) :-   (Se=[osoba|_];Se=[zviera|_]),   (Sf='';Sf=ka;Sf='iËka';Sf='yÚa';Sf=ina;Sf=ica;Sf='ovn·';Sf='aÚa';Sf='ezn·'),   radix(KO,_,_,cat(_,_,_,Vz,_)),code_world(W,main),vzorsub(Vz,Rod,_),   code_world(_,W),Rod\=zen,   disfar(KO,L1),ctr_inc(1,Y),attach(L1,[(ifr,0),[Y,('ûen',0)]],L).% vÂËa,sÙv‰,ûieÚainval(km(sub,S,_,Sf,_,KM),ko(sub,S,_,KO),L) :-   (Sf=a;Sf='‰';Sf='Ëa'),(S=[osoba|_];S=[zviera|_]),   disfar(KO,L1),ctr_inc(1,X),attach(L1,[(atr,0),[X,(mal,1)]],L).% vl·knina,baranina,sudovinainval(km(sub,[mat|_],_,Sf,_,KM),ko(sub,Se,_,KO),L) :-   (Sf=ina;Sf=ovina),   ((Se=[mat|_],SP=rel);(Se=[zviera|_],SP=suk);(sema(Se,[kon]),SP=mot)),   disfar(KO,L1),ctr_inc(1,X),attach([[X,('l·tk',0)]],[(SP,0)|L1],L).% kotlinainval(km(sub,[lok|_],_,Sf,_,KM),ko(sub,Se,_,KO),L) :-   (Sf=ina;Sf=ovina),sema(Se,[kon]),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(miest,0)]],[(rel,0)|L1],L).% zvonica,kvetin·Ë,ËajnÌk,rybnÌk,sklen·rstvo,% ûeleziareÚ,nevestinec,kapustnisko,vËelÌn,kÙlÚainval(km(sub,[lok|_],_,Sf,_,KM),ko(sub,_,_,KO),L) :-   (Sf=ica;Sf='·Ë';Sf='nÌk';Sf=stvo;Sf=tvo;Sf='eÚ';Sf='·reÚ';Sf='iareÚ';    Sf='Úa';Sf='ovÚa';Sf=nisko;Sf=isko;Sf=ovisko;Sf=nica;Sf='Ìn';Sf=inec),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(miest,0)]],[(xlok,_)|L1],L).% studnica,kruûnica,hrobkainval(km(sub,S,_,Sf,_,KM),ko(sub,S,_,KO),L) :-   (Sf=ica;Sf=nica;Sf=ka;Sf=ce),   disfar(KO,L).% zemiak,prsteÚ,vr·tnica,n·prsenkainval(km(sub,S,Pr,Sf,_,KM),ko(sub,S2,_,KO),L) :-   (Sf=ka;Sf=nica;Sf=enka;Sf=ok;Sf='nÌk';Sf=iak;Sf=ak;Sf='·k';Sf='eÚ'),   (S2=[vec|_];S2=[lok|_]),int4(Pr,lok,No),taxon(KM,S,Nam),   disfar(KO,L1),ctr_inc(1,X),attach([[X,Nam]],[(lok,No)|L1],L).		% slneËnÌk,figovnÌk,makovnÌk,dennÌk,hlinÌk,vrtulnÌk% sÌran,krveæ,kamenec,olovnica,guæovnica,z·vitnicainval(km(sub,S,_,Sf,_,KM),ko(sub,S2,_,KO),L) :-   (Sf='nÌk';Sf=nica;Sf='eæ';Sf=an;Sf=ec;Sf='Ìk'),   sema(S,[kon]),taxon(KM,S,Nam),   disfar(KO,L1),ctr_inc(1,X),attach([[X,Nam]],[(koh,0)|L1],L).% jelöina,malinËie,kanaliz·ciainval(km(sub,S,_,Sf,_,KM),ko(sub,S,_,KO),L) :-   (Sf=ie;Sf=ina;Sf=stvo;Sf='iz·cia';Sf='aù';Sf='eù';Sf='aË';Sf='iË'),   radix(KM,KO,sub,cat(_,_,_,_,hrom)),   disfar(KO,L1),attachh(L1,[mes,'veæa'],L).% plech·Ëinval(km(sub,S,_,'·Ë',_,KM),ko(sub,[mat|_],_,KO),L) :-   sema(S,[kon]),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(vec,0)]],[(mat,0)|L1],L).% Zadunajsko, povaûie, predhorieinval(km(sub,[lok|_],Pr,Sf,_,KM),ko(sub,[lok|_],_,KO),L) :-   (Sf=sko;Sf=ie),   disfar(KO,L1),ctr_inc(1,X),int4(Pr,lok,No),   attach([[X,(miest,0)]],[(lok,No)|L1],L).% predlaktie, z·p‰stieinval(km(sub,[korp|_],Pf,ie,_,KM),ko(sub,[korp|_],_,KO),L) :-   int4(Pr,lok,No),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(vec,0)]],[(lok,No)|L1],L).% s˙hvezdie, s˙soöieinval(km(sub,Se1,'s˙',ie,_,_),ko(sub,Se2,_,KO),L) :-   disfar(KO,L1),ctr_inc(1,X),attach(L1,[(kar,0),[X,('veæa',1)]],L).% polmesiac,veæchr·minval(km(sub,S,Pr,'',_,KM),ko(sub,S,_,KO),L) :-    (((Pr=pol;Pr=polo),Atr=('poloviËn',1));     (Pr='veæ',Atr=('veæk',1))),    disfar(KO,L1),ctr_inc(1,X),attach(L1,[(atr,0),[X,Atr]],L).% Ëlnok,biËÌk,kab·tec,slnieËkoinval(km(sub,S1,_,Sf,_,KM),ko(sub,S2,_,KO),L) :-   (Sf=ok;Sf='Ìk';Sf=ik;Sf=ec;Sf='Ëek';Sf=ek;Sf=tek;Sf='ÌËek';Sf='ieËko';    Sf='eËko';Sf='oËko';Sf='ÌËko';Sf='Ëok';Sf=ka;Sf='iËka';Sf='oËka';    Sf='ÙËka';Sf=enka;Sf=ienka;Sf=inka;Sf=ko;Sf=ce;Sf=inko;Sf=enko),   ifthen(S1=[osoba|_],Sf\==ka),(S1=S2;(S1=[vec|_],S2=[mat|_])),   disfar(KO,L1),ctr_inc(1,X),attach(L1,[(atr,0),[X,(mal,1)]],L).% stromiskoinval(km(sub,S,_,Sf,_,KM),ko(sub,S,_,KO),L) :-   (Sf=isko;Sf=sko),   disfar(KO,L1),ctr_inc(1,X),attach(L1,[(atr,0),[X,('veæk',1)]],L).% podtext,protin·vrh% ADJ -> SUB% slepec,dravec,podnikavec,bruch·Ë,listn·Ë,dobr·k,smrteænÌk% strieborniak,st·lica,drevenica,beluöa,starena,tekutinainval(km(sub,Se,_,Sf,_,KM),ko(adj,S2,_,KO),L) :-   (Sf='·Ë';Sf=ec;Sf='nÌk';Sf='·k';Sf=ak;Sf=iak;Sf='Ìk';Sf=an;    Sf='uöa';Sf=ucha;Sf='uæa';Sf=ena;Sf=ina),   taxon(KM,Se,Sen),disfar(KO,L1),ctr_inc(1,X),   attach([[X,Sen]],[(atr,0)|L1],L).% Ëernoch,skup·Ú,bleduæa,zlatoÚinval(km(sub,Se,_,Sf,_,KM),ko(adj,_,_,KO),L) :-   (Sf='·Ë';Sf='·Ú';Sf='oÚ';Sf='·ö';Sf='·æ';Sf=och),   disfar(KO,L1),ctr_inc(1,X),taxon(KM,Se,Nam),   attachh(L1,[mod,charakt],L2),attach([[X,Nam]],[(atr,0)|L2],L).% zlatovl·ska, bielohl·vok, voÚavkainval(km(sub,Se,'',Sf,_,KM),ko(adj,_,_,KO),L) :-   (Sf=ok;Sf=ka;Sf='·Ú'),   taxon(KM,Se,Nam),   leftval(KO,ano,poz,Nam,Q),ctr_inc(1,X),ctr_inc(1,Y),   attach([[X,Nam]],Q,Q1),   ifthenelse((Sf=ka,sema(Se,[ziv])),attach(Q1,[(ifr,0),[Y,('ûen',0)]],L),L=Q1).% str·ûny,sl˙ûne% hlavn˝% husacinainval(km(sub,[mat|_],_,Sf,_,KM),ko(adj,drprivl,_,KO),L) :-   (Sf=ina;Sf=ovina),radix(KO,KOR,sub,_),   disfar(KOR,L1),ctr_inc(1,X),attach([[X,('l·tk',0)]],[(suk,0)|L1],L).% franc˙zötinainval(km(sub,[gnoz|_],_,Sf,_,KM),ko(adj,akvzt,_,KO),L) :-   (Sf=ina;Sf='Ëina';Sf='ötina'),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(jazyk,0)]],[(atr,0)|L1],L).% hÂbka,ËerÚ,v˝öava,sladkosù,öialenstvoinval(km(sub,[atb|_],Pf,Sf,_,KM),ko(adj,_,_,KO),L) :-   (Sf='';Sf=a;Sf='osù';Sf=oba;Sf=ota;Sf=tvo;Sf=ctvo;Sf=stvo;Sf=ava;Sf=izmus),   (Pf='';Pf=ne;Pf=polo;Pf=bez),   disfar(KO,L).							       % drev·rstvo,starodievoctvoinval(km(sub,[jav|_],'',Sf,_,_),ko(sub,_,_,KO),L) :-   (Sf=stvo;Sf=ctvo;Sf=tvo),   disfar(KO,L1),ctr_inc(1,X),attach([[X,(jav,0)]],[(koh,0)|L1],L).% chorobnosù, ˙razovosù% SUB -> SL% kraæovaù,chalup·rËiùinval(km(sl,400,'',Sf,nedok,_),ko(sub,[osoba|_],_,KO),L) :-   (Sf='ovaù';Sf='Ëiù'